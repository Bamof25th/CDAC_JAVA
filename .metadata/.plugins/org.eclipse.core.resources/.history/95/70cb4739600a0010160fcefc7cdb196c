package com.banking.main;

public class BankAccount {

	private int acno;
	private String fName;
	private String lName;
	private double balance;
	private String accType;

	public BankAccount(int acno, String fName, String lName, double balance, String accType) {
		super();
		this.acno = acno;
		this.fName = fName;
		this.lName = lName;
		this.balance = balance;
		this.accType = accType;
	}

	@Override
	public String toString() {
		return "BankAccount [acno=" + acno + ", fName=" + fName + ", lName=" + lName + ", balance=" + balance
				+ ", accType=" + accType + "]";
	}

	public double getBalance() {
		return balance;
	}
	
	public int getAcno() {
		return acno;
	}

	public void setAcno(int acno) {
		this.acno = acno;
	}

	public String getfName() {
		return fName;
	}

	public void setfName(String fName) {
		this.fName = fName;
	}

	public String getlName() {
		return lName;
	}

	public void setlName(String lName) {
		this.lName = lName;
	}

	public String getAccType() {
		return accType;
	}

	public void setAccType(String accType) {
		this.accType = accType;
	}

	public void setBalance(double balance) {
		this.balance = balance;
	}

	public void AddBalance(int paise) {
		this.balance += paise ;
		System.out.println("Balance Update : "+ this.balance);
	}

}
